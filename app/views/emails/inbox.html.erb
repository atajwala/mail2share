<h1>Emails#inbox</h1>
<table>
  <tr>
    <th>Userid</th>
    <th>Username</th>
    <th>Subject</th>
    <th>Sender email</th>
    <th>Date</th>
  </tr>

<% @emails.each do |email| %>
  <tr>
    <td><%= email.user_id %></td>
    <td><%= email.username %></td>
    <td><%= link_to "#{email.subject}", { :action => "messages", :controller => "emails", :file_key => "#{email.file_key}" } %> </td>
    <td><%= email.sender_email %></td>
    <td><%= email.created_at %></td>
  </tr>
<% end %>
</table>
